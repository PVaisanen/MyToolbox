<h2 class="page-header">Search Toolboxes</h2>
<form action="/toolboxes" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%=searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% toolboxes.forEach(toolbox => { %> 
    <div><%=toolbox.name %></div>
        <div class="btn-row">
            <a class="btn btn-primary" href="/toolboxes/<%= toolbox.id %>">View</a>
            <a class="btn btn-primary" href="/toolboxes/<%= toolbox.id %>/edit">Edit</a>
            <%- include('../partials/deleteForm', 
            { url: `/toolboxes/${toolbox.id}` }) %>
        </div>

<% })%> 